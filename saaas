import random
NUN = random.randrange(1,100)
total = 10

while(total > 0):
    laal_str = input("DIGITE SEU NÚMERO: ")
    print("VOCÊ DIGITOU" ,laal_str)
    laal = int(laal_str)
    print(total, "tentativas restantes")
    certou = laal == NUN
    maior  = laal > NUN
    menor  = laal < NUN

    if(certou):
        print("{[[{{{[{[{[{[{[{[{[{[{ACERTOU}]}]}]}]}]}]}]}]}}}]]}")
        print("█████████████▀▀▀▀▀███████▀▀▀▀▀█████████████");
